{
  "name": "afghan-pay-top-up-api",
  "version": "1.0.0",
  "description": "AfghanPay Top-Up API",
  "main": "src/server.js",
  "scripts": {
    "local": "set NODE_ENV=local&& nodemon src/server.js",
    "dev": "NODE_ENV=development pm2 start src/server.js --watch 'src/' --ignore-watch 'src/uploads'",
    "stage": "NODE_ENV=stage pm2 start src/server.js --watch 'src/' --ignore-watch 'src/uploads'",
    "pro": "NODE_ENV=production pm2 start src/server.js",
    "pro-cluster": "NODE_ENV=production node src/server.js",
    "start": "node src/server.js",
    "start-cluster": "NODE_ENV=production node src/server.js",
    "test-performance": "npx autocannon -c 100 -d 30 http://localhost:5000/api",
    "load-test:quick": "node src/load-test-master.js --quick",
    "load-test:full": "node src/load-test-master.js",
    "load-test:stress": "node src/load-test-master.js --stress",
    "load-test:cluster": "node src/load-test-master.js --cluster",
    "load-test:rabbitmq": "node src/load-test-master.js --rabbitmq",
    "test:performance": "npx autocannon -c 100 -d 30 http://localhost:5000/api/health",
    "load-test:etisalat": "node src/load-test-etisalat.js",
    "load-test:etisalat-quick": "node src/load-test-etisalat.js --quick",
    "load-test:etisalat-stress": "node src/load-test-etisalat.js --stress",
    "load-test:5000rps": "node src/load-test-5000rps.js",
    "load-test:1000rps": "node src/load-test-5000rps.js --quick",
    "load-test:10000rps": "node src/load-test-5000rps.js --extreme",
    "load-test:production": "NODE_ENV=production node src/load-test-5000rps.js"
  },
  "keywords": [],
  "author": "Exioms",
  "license": "ISC",
  "dependencies": {
    "aes256": "^1.1.0",
    "amqplib": "^0.8.0",
    "aws-sdk": "^2.1009.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-random-string": "^4.0.0",
    "dotenv": "^9.0.2",
    "exceljs": "^4.4.0",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "fast-csv": "^5.0.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "multer": "^1.4.2",
    "mysql2": "^3.14.0",
    "node-cron": "^3.0.0",
    "node-dataset": "^1.3.42",
    "pdfkit": "^0.17.2",
    "randomstring": "^1.2.1",
    "redis": "^3.1.2",
    "redis-connection-pool": "^1.7.4",
    "request": "^2.88.2",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "autocannon": "^7.15.0",
    "eslint": "^7.26.0",
    "nodemon": "^2.0.14"
  }
}
